<?xml version="1.0" encoding="UTF-8" ?> 
<Module>
  <ModulePrefs title="The Example Help Desk Gadget"
        author="User"
        author_email="user@domain.com"
  /> 
  <UserPref name="displayedURL" display_name="URL to display" required="true"></UserPref>
  <Content type="html" >
     <![CDATA[ 

     <div id="debug_div" style="font-size:9pt; padding:5px; color: red;"></div>

     <script type="text/javascript">
      var dd = new Date();
      var hh = dd.getHours();
      var mm = dd.getMinutes();
      var ss = dd.getSeconds();
      document.getElementById("debug_div").innerHTML = "Start - " + hh + ':' + mm + ':' + ss;
     </script>

     <ul>
     <li><a href="http://www.google.com" target="_blank">Help Site</a>
     <li><a href="http://www.google.com" target="_blank">Open a Ticket</a>
     <li><a href="mailto:http://www.google.com">Email Support</a>
     </ul>

     <script type="text/javascript">
      var dd = new Date();
      var hh = dd.getHours();
      var mm = dd.getMinutes();
      var ss = dd.getSeconds();
      document.getElementById("debug_div").innerHTML += " End - " + hh + ':' + mm + ':' + ss;
     </script>

     ]]>
  </Content> 
</Module>
